{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "kypo-sandbox-model": {
            "projectType": "library",
            "root": "projects/kypo-sandbox-model",
            "sourceRoot": "projects/kypo-sandbox-model/src",
            "prefix": "kypo",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/kypo-sandbox-model/tsconfig.lib.json",
                        "project": "projects/kypo-sandbox-model/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/kypo-sandbox-model/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/kypo-sandbox-model/src/test.ts",
                        "tsConfig": "projects/kypo-sandbox-model/tsconfig.spec.json",
                        "karmaConfig": "projects/kypo-sandbox-model/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/kypo-sandbox-model/**/*.ts",
                            "projects/kypo-sandbox-model/**/*.component.html"
                        ]
                    }
                }
            }
        }
    },
    "schematics": {
        "@angular-eslint/schematics:application": {
            "setParserOptionsProject": true
        },
        "@angular-eslint/schematics:library": {
            "setParserOptionsProject": true
        }
    },
    "cli": {
        "analytics": false
    }
}
